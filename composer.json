{
    "name": "vanthao03596/laravel-ghtk",
    "description": "This is my package LaravelGhtk",
    "keywords": [
        "vanthao03596",
        "laravel",
        "laravel-ghtk"
    ],
    "homepage": "https://github.com/vanthao03596/laravel-ghtk",
    "license": "MIT",
    "authors": [
        {
            "name": "phamthao",
            "email": "phamthao03596@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "graham-campbell/manager": "^4.5",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "toin0u/digitalocean-v2": "4.3.*"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Vanthao03596\\LaravelGhtk\\": "src",
            "Vanthao03596\\LaravelGhtk\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanthao03596\\LaravelGhtk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vanthao03596\\LaravelGhtk\\LaravelGhtkServiceProvider"
            ],
            "aliases": {
                "LaravelGhtk": "Vanthao03596\\LaravelGhtk\\LaravelGhtkFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
